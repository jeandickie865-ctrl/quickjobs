{
  "expo": {
    "name": "Quickjobs",
    "slug": "backup-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/store-icons/app-icon.png",
    "scheme": "backup",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "de.dickie.backup",
      "buildNumber": "2",
      "supportsTablet": false,
      "appStoreUrl": "https://apps.apple.com/app/id6756327012",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "SKU": "quickjobs001",
        "appleAppId": "6756327012"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/store-icons/app-icon.png",
        "backgroundColor": "#141126"
      },
      "edgeToEdgeEnabled": true,
      "package": "de.dickie.backup"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "lang": "de",
      "meta": {
        "google": "notranslate"
      }
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://backup.dickie.app"
        }
      ],
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "backendUrl": "",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6bf49238-ee15-46b2-bc4a-253d1c903563"
      }
    },
    "owner": "backupkj"
  }
}
